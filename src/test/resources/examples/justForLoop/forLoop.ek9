#!ek9
defines module just.forloops.check

  defines program

    ForLoopExample1
      stdout <- Stdout()

      for i in 1 ... 10
        stdout.println(`Value [${i}]`)

    ForLoopExample2
      stdout <- Stdout()

      for i in 0 ... 10 by 2
        stdout.println(`Value [${i}]`)

    ForLoopExample3
      stdout <- Stdout()

      j <- 2
      for i in 0 ... 10 by j
        stdout.println(`Value [${i}]`)

    ForLoopExample4
      stdout <- Stdout()

      j <- -2
      for i in 10 ... 0 by j
        stdout.println(`Value [${i}]`)

//EOF